@startuml "Edit/Add Component"
    left to right direction

    actor  "User" as user

    rectangle "System" {
        usecase "New Component" as new
        usecase "Edit Component" as edit
        usecase "Update Component" as update
        usecase "Get Component" as get
    }

    user -- new
    user --edit
    edit -- update
    edit -- get

@enduml